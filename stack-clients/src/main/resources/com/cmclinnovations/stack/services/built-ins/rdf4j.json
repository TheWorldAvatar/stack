{
    "type": "rdf4j",
    "ServiceSpec": {
        "Name": "rdf4j",
        "TaskTemplate": {
            "ContainerSpec": {
                "Image": "ghcr.io/theworldavatar/eclipse/rdf4j-workbench:5.3.0-timeouts-tomcat-SNAPSHOT",
                "Mounts": [
                    {
                        "Type": "volume",
                        "Source": "rdf4j-data",
                        "Target": "/var/rdf4j"
                    },
                    {
                        "Type": "volume",
                        "Source": "rdf4j-logs",
                        "Target": "/usr/local/tomcat/logs"
                    }
                ],
                "Healthcheck": {
                    "Test": [
                        "CMD",
                        "curl",
                        "-f",
                        "http://localhost:8080/rdf4j-server"
                    ],
                    "Interval": 1000000000,
                    "Timeout": 1000000000,
                    "Retries": 50,
                    "StartPeriod": 3000000000
                }
            }
        }
    },
    "endpoints": {
        "workbench": {
            "url": "http://localhost:8080/rdf4j-workbench/",
            "externalPath": "/rdf4j-workbench/"
        },
        "server": {
            "url": "http://localhost:8080/rdf4j-server/",
            "externalPath": "/rdf4j-server/"
        },
        "query": {
            "url": "http://localhost:8080/rdf4j-server/repositories/stack-incoming",
            "externalPath": "/sparql/query"
        }
    }
}