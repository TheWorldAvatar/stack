            "type": "Tabular",
            "skip": false,
            "schema": "public",
            "subdirectory": "tabular"
        },
        {
            "type": "Vector",
            "skip": false,
            "schema": "public",
            "table": "cropmap_bed",
            "subdirectory": "Bedfordshire",
            "ogr2ogrOptions": {
                "otherOptions": {
                    "-select": [
                        "prob,county,cromeid,lucode"
                    ]
                },
                "layerCreationOptions": {
                    "GEOMETRY_NAME": "geom",
                    "FID": "fid"
                }
            },
            "geoServerSettings": {
                "virtualTable": {
                    "name": "cropmap_extended_bed",
                    "sql": "SELECT fid, prob, county, cromeid, cropmap_bed.lucode, geom, label, colour\nFROM cropmap_bed, cropmap_colours, cropmap_labels\nWHERE cropmap_bed.lucode = cropmap_labels.lucode AND cropmap_bed.lucode = cropmap_colours.lucode\n",
                    "escapeSql": false,
                    "keyColumn": "label",
                    "geometry": {
                        "name": "geom",
                        "type": "Polygon",
                        "srid": 27700
                    }
                },
                "defaultStyle": "the_world_avatar:cropmap_simplified"
            }
        },
